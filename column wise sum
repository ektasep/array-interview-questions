#include <iostream>

using namespace std;
void colwise(int arr[][3],int row,int col){
    int sum=0;
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            sum=sum+arr[j][i];
            
        }
        cout<<endl;
    }
    cout<<sum<<" ";
    
}

int main()
{
int arr[3][3];
int row=3;
int col=3;
for(int i=0;i<row;i++){
    for(int j=0;j<col;j++){
        cin>>arr[j][i];
    }
    cout<<endl;
}
for(int i=0;i<row;i++){
    for(int j=0;j<col;j++){
        cout<<arr[i][j];
    }
    cout<<endl;
}
colwise(arr,row,col);

    return 0;
}
