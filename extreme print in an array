#include <iostream>

using namespace std;



int main()
{
    int arr[100]={10,20,30,40,50,60,70,80};
    int size=8;
    int i=0;
    int j=size-1;
    while(true){
        if(i>j){
            break;
        }
    cout<<arr[i]<<" ";
    cout<<arr[j]<<" ";

    i++;
    j--;
        }
    //printing the array    
    //for(int i=0;i<size;i++){
       // cout<<arr[i];
  //  }
        
    
    return 0;

}
