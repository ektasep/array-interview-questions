#include <iostream>

using namespace std;
bool linearsearch(int arr[],int size,int key){
    for(int i=0;i<size;i++){
        if(arr[i]==key){
            return true;
        }
    }
    return false;
}

int main()
{
int arr[100]={100,2,300,400,500,98};
int size=5;
int target=5;
int ans=linearsearch(arr,size,target);
if(ans==1){
    cout<<"key is found    "<<arr[ans];
    
}
else{
    cout<<"key is not found";
}
return 0;
}
