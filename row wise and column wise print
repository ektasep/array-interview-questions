//rowwise
#include <iostream>

using namespace std;

int main()
{
    //take the input from the user
    int arr[3][3];
    int row=3;
    int col=3;
    //outer loop for row
    for(int i=0;i<row;i++){
        //inner loop for col
        for(int j=0;j<col;j++){
            cin>>arr[i][j];
        }
    }
    // print the output
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cout<<arr[i][j]<<" ";
        }
    cout<<endl;
    }
   

    return 0;
}

//col wise print

#include <iostream>

using namespace std;

int main()
{
    //take the input from the user
    int arr[3][3];
    int row=3;
    int col=3;
    //outer loop for row
    for(int i=0;i<row;i++){
        //inner loop for col
        for(int j=0;j<col;j++){
            cin>>arr[j][i];
        }
    }
    // print the output
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cout<<arr[i][j]<<" ";
        }
    cout<<endl;
    }
   

    return 0;
}
